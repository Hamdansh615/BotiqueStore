{
    "name": "react_template",
    "version": "0.0.1",
    "buildNumber": "1",
    "private": true,
    "scripts": {
        "android": "react-native run-android run-android --verbose",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "clean": "yarn run cleanBuildAndroid && yarn run cleanBuildIOS",
        "debugApk": "yarn run clean && cd android && ./gradlew assembleDebug && cd ..",
        "releaseApk": "yarn run clean && cd android && ./gradlew assembleRelease && cd ..",
        "test": "jest",
        "updateVersion": "node node_modules/react-native-version-env-manager --updateVersion",
        "env:local": "node node_modules/react-native-version-env-manager --env=local --envFileExt=ts --addEnvAsPostfix",
        "env:dev": "node node_modules/react-native-version-env-manager --env=dev --envFileExt=ts --addEnvAsPostfix",
        "env:qa": "node node_modules/react-native-version-env-manager --env=qa --envFileExt=ts --addEnvAsPostfix",
        "env:stag": "node node_modules/react-native-version-env-manager --env=stag --envTag=staging --envFileExt=ts --addEnvAsPostfix",
        "env:prod": "node node_modules/react-native-version-env-manager --env=prod --envFileExt=ts",
        "prepare": "husky install",
        "lint": "eslint .",
        "lint:fix": "eslint \"**/**.{ts,tsx}\" --fix",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.19.3",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/stack": "^6.0.7",
        "@reduxjs/toolkit": "^1.9.7",
        "apisauce": "^3.0.1",
        "core-util-is": "^1.0.3",
        "formik": "^2.4.5",
        "global": "^4.4.0",
        "husky": "^8.0.3",
        "pod-install": "^0.1.39",
        "prettier": "^3.0.3",
        "pretty-ms": "^8.0.0",
        "react": "18.2.0",
        "react-native": "0.72.3",
        "react-native-confirmation-code-field": "^7.3.2",
        "react-native-gesture-handler": "^2.13.1",
        "react-native-keychain": "^8.1.2",
        "react-native-phone-input": "^1.3.4",
        "react-native-progress": "^5.0.0",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-screens": "^3.25.0",
        "react-native-simple-toast": "^3.0.1",
        "react-native-svg": "^12.1.0",
        "react-native-vector-icons": "^10.0.0",
        "react-native-version-check": "^3.4.7",
        "react-native-webview": "^11.6.5",
        "react-redux": "^8.1.3",
        "redux-thunk": "^2.4.2",
        "toggle-switch-react-native": "^3.3.0",
        "yup": "^1.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@expo/metro-config": "^0.10.7",
        "@react-native-community/eslint-config": "^3.2.0",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.11",
        "@testing-library/jest-native": "^5.4.2",
        "@testing-library/react-native": "^7.1.0",
        "@tsconfig/react-native": "^3.0.0",
        "@types/jest": "^29.4.0",
        "@types/react-native": "^0.72.3",
        "@types/react-native-check-box": "^2.1.2",
        "@types/react-test-renderer": "^18.0.0",
        "@types/redux-logger": "^3.0.10",
        "@types/yup": "^0.29.6",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "babel-jest": "^29.4.3",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.4.3",
        "lint-staged": "^11.1.2",
        "metro-react-native-babel-preset": "0.76.8",
        "react-native-svg-transformer": "^1.0.0",
        "react-native-version-env-manager": "^1.2.4",
        "react-test-renderer": "18.2.0",
        "redux-logger": "^3.0.6",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=16"
    },
    "resolutions": {
        "@types/react": "^18.0.24"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
